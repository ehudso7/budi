{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "pnpm turbo run build --filter=@budi/api",
  "installCommand": "pnpm install",
  "outputDirectory": "public",
  "framework": null,
  "functions": {
    "services/api/api/index.ts": {
      "memory": 1024,
      "maxDuration": 30,
      "includeFiles": "services/api/dist/**"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}
