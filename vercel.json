{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "installCommand": "pnpm install",
  "buildCommand": "pnpm turbo run build --filter=@budi/api",
  "builds": [
    {
      "src": "services/api/api/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["services/api/dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/services/api/api/index.ts"
    }
  ]
}
